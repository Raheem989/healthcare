@model IEnumerable<FinalProjectKidsHealthCenter.Models.VaccineTable>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DissessesTable.Di_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VaccineAgeTable.VaccineAgeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VaccineNameTable.VacineName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoseRoute)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
