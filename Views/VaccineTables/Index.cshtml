            @Html.DisplayFor(modelItem => item.DissessesTable.Di_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VaccineAgeTable.VaccineAgeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VaccineNameTable.VacineName)
        </td>
        <td>
